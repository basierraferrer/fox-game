{
  "browserslist": [
    "last 2 Firefox versions"
  ],
  "name": "fox-game",
  "version": "1.0.0",
  "description": "Version of a virtual pet game",
  "main": "src/index.html",
  "scripts": {
    "clean:dev": "rm -rf .cache dist",
    "dev": " yarn clean:dev && parcel src/index.html",
    "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,css}\"",
    "format:check": "prettier --ignore-path ./.gitignore --check \"./**/*.{html,json,js,ts,css}\"",
    "lint": "eslint --ignore-path ./.gitignore --fix \"./**/*.{js,ts}\"",
    "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basierraferrer/fox-game.git"
  },
  "keywords": [
    "game",
    "javascript"
  ],
  "author": "Brian Sierra <basierraferrer@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/basierraferrer/fox-game/issues"
  },
  "homepage": "https://github.com/basierraferrer/fox-game#readme",
  "devDependencies": {
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.3.3"
  }
}
